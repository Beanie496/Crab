FUNC main():
    uci_main_loop()
ENDFUNC

FUNC uci_main_loop():
    DO:
        line = readline()
        line.split_on(" ")
    WHILE handle_uci_commands(line) != QUIT
ENDFUNC

FUNC handle_uci_commands(line):
    SWITCH line[0]:
        "uci":
            // do nothing
        "setoption":
            handle_setoption(line)
        // ...
        "quit":
            return QUIT
    ENDSWITCH
ENDFUNC
